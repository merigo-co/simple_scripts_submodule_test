{
  "Test": {
    "access": {
      "game": true,
      "player": true
    },
    "commands": [
      {
        "display": "Set test mode",
        "command": "set_test_mode",
        "buttonDisplay": "Set test mode",
        "args": [
          {
            "name": "test_mode",
            "display": "Test mode",
            "type": "List",
            "values": ["on", "off"],
            "default": "off"
          }
        ]
      }
    ]
  },
  "Printer": {
    "access": {
      "game": true,
      "player": true
    },
    "commands": [
      {
        "display": "Print Debug Msg",
        "command": "print_debug",
        "buttonDisplay": "Run",
        "args": [{"name": "text", "display": "text", "default": ""}]
      },
      {
        "display": "Print Info Msg",
        "command": "print_info",
        "buttonDisplay": "Run",
        "args": [{"name": "text", "display": "text", "default": ""}]
      },
      {
        "display": "Print Warn Msg",
        "command": "print_warn",
        "buttonDisplay": "Run",
        "args": [{"name": "text", "display": "text", "default": ""}]
      },
      {
        "display": "Print Error Msg",
        "command": "print_error",
        "buttonDisplay": "Run",
        "args": [{"name": "text", "display": "text", "default": ""}]
      },
      {
        "display": "Print Msg",
        "command": "print_msg",
        "buttonDisplay": "Run",
        "args": [
          {
            "name": "type",
            "display": "type",
            "type": "List",
            "values": ["debug", "info", "warn", "error"],
            "default": "debug"
          },
          {"name": "text", "display": "text", "default": ""}
        ]
      },
      {
        "display": "Print Msg Enum",
        "command": "print_msg_enum",
        "buttonDisplay": "Run",
        "args": [
          {
            "name": "type",
            "display": "type",
            "type": "KV",
            "values": [
              ["debug", "0"],
              ["info", "1"],
              ["warn", "2"],
              ["error", "3"]
            ],
            "default": "debug"
          },
          {"name": "text", "display": "text", "default": ""}
        ]
      }
    ]
  },
